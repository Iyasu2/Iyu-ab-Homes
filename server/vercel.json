{
  "version": 2,
  "builds": [
    {
      "src": "routes/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "uploads/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    }
  ]
}
